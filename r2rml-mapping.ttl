@prefix :       <https://behindertenverband-leipzig.de/ns/r2rml/> .
@prefix acc:    <https://github.com/AKSW/leds-asp-f-ontologies/raw/master/ontologies/place-accessibility/ontology.ttl#> .
@prefix p:      <https://github.com/AKSW/leds-asp-f-ontologies/raw/master/ontologies/place/ontology.ttl#> .
@prefix bvl:    <https://behindertenverband-leipzig.de/ns/gebaeude/> .
@prefix dc:     <http://purl.org/dc/elements/1.1/> .
@prefix dct:    <http://purl.org/dc/terms/> .
@prefix dbp:    <http://dbpedia.org/ontology/> .
@prefix geo:    <http://www.w3.org/2003/01/geo/> .
@prefix rr:     <http://www.w3.org/ns/r2rml#> .
@prefix schema: <http://schema.org/> .
@prefix wa:     <http://semweb.mmlab.be/ns/wa#> .

#
# This file contains a R2RML mapping. It is used to transform table-like data to RDF structures.
#

:r2rml
    # target table (in this case the CSV file)
    rr:logicalTable [
        rr:tableName "\"le-online-extracted-places.csv\""
    ] ;
    # subject information
    rr:subjectMap [
        rr:class p:Place ;
        rr:template "https://behindertenverband-leipzig.de/places/{ID}" ;
        rr:termType rr:IRI
    ] ;

    rr:predicateObjectMap [
        rr:objectMap [ rr:column "\"ID\"" ] ;
        rr:predicate dct:identifier
    ] ;

    rr:predicateObjectMap [
        rr:objectMap [ rr:column "\"Kategorie\"" ] ;
        rr:predicate dbp:category
    ] ;

    rr:predicateObjectMap [
        rr:objectMap [ rr:column "\"Titel\"" ] ;
        rr:predicate dc:title
    ] ;

    rr:predicateObjectMap [
        rr:objectMap [ rr:column "\"Oeffnungszeiten\"" ] ;
        rr:predicate p:openingHours
    ] ;

    rr:predicateObjectMap [
        rr:objectMap [ rr:column "\"Beschreibung-Hilfestellungen-vor-Ort\"" ] ;
        rr:predicate acc:localSupport
    ] ;

    rr:predicateObjectMap [
        rr:objectMap [ rr:column "\"Besondere-Hilfestellungen-f-Menschen-m-Hoerbehinderung-vorhanden\"" ] ;
        rr:predicate acc:supportForHearingImpairedAvailable
    ] ;

    rr:predicateObjectMap [
        rr:objectMap [ rr:column "\"Besondere-Hilfestellungen-f-Menschen-m-Seebhind-Blinde-vorhanden\"" ] ;
        rr:predicate acc:supportForVisuallyImpairedAvailable
    ] ;

    rr:predicateObjectMap [
        rr:objectMap [ rr:column "\"Anzahl-Behindertenparkplaetze-v-Einrichtung\"" ] ;
        rr:predicate acc:numberOfSlotsDisabledParkingLotInFrontOfPlace
    ] ;

    rr:predicateObjectMap [
        rr:objectMap [ rr:column "\"Anzahl-Behindertenparkplaetze-auf-hauseigenem-Parkplatz\"" ] ;
        rr:predicate acc:numberOfSlotsDisabledParkingLotOnSite
    ] ;

    rr:predicateObjectMap [
        rr:objectMap [ rr:column "\"Hauseigener-Parkplatz-vorhanden\"" ] ;
        rr:predicate p:parkingLotOnSiteAvailable
    ] ;

    rr:predicateObjectMap [
        rr:objectMap [ rr:column "\"Ort-hauseigener-Parkplatz\"" ] ;
        rr:predicate p:locationParkingLotOnSite
    ] ;

    #
    # References ------------------------------------------------------------------------------
    #

    # Reference to Elevator entity
    rr:predicateObjectMap [
        rr:objectMap [ rr:template "https://behindertenverband-leipzig.de/places/{ID}-elevator" ] ;
        rr:predicate p:elevator
    ] ;

    # Reference to Entrance entity
    rr:predicateObjectMap [
        rr:objectMap [ rr:template "https://behindertenverband-leipzig.de/places/{ID}-entrance" ] ;
        rr:predicate p:entrance
    ] ;

    # Reference to Location entity
    rr:predicateObjectMap [
        rr:objectMap [ rr:template "https://behindertenverband-leipzig.de/places/{ID}-location" ] ;
        rr:predicate p:location
    ] ;

    # Reference to Toilet entity
    rr:predicateObjectMap [
        rr:objectMap [ rr:template "https://behindertenverband-leipzig.de/places/{ID}-toilet" ] ;
        rr:predicate p:toilet
    ] .

#
#                   p:location                                geo:long
# bvl:SomePlace -------------> bvl:SomePlace/Location -----------------------> ...
#                                                             geo:lat
#                                                           -----------------------> ...
# ...
#
[] a rr:TriplesMap;

    rr:logicalTable [
        rr:tableName "\"le-online-extracted-places.csv\""
    ] ;

    rr:subjectMap [
        rr:class p:Location ;
        rr:template "https://behindertenverband-leipzig.de/places/{ID}-location" ;
        rr:termType rr:IRI
    ] ;

    rr:predicateObjectMap [
        rr:objectMap [ rr:column "\"Longitude\"" ] ; rr:predicate geo:long
    ] ;

    rr:predicateObjectMap [
        rr:objectMap [ rr:column "\"Latitude\"" ] ; rr:predicate geo:lat
    ] ;

    # Address
    rr:predicateObjectMap [
        rr:objectMap [ rr:column "\"Strasse\"" ] ; rr:predicate schema:streetAddress
    ] ;
    rr:predicateObjectMap [
        rr:objectMap [ rr:column "\"PLZ\"" ] ; rr:predicate schema:postalCode
    ] ;
    rr:predicateObjectMap [
        rr:objectMap [ rr:column "\"Ort\"" ] ; rr:predicate schema:addressLocality
    ] .

#
#                   p:entrance
# bvl:SomePlace -------------> bvl:SomePlace/Entrance -----------------------> ...
# ...
#
[] a rr:TriplesMap;

    rr:logicalTable [
        rr:tableName "\"le-online-extracted-places.csv\""
    ] ;

    rr:subjectMap [
        rr:class p:Entrance ;
        rr:template "https://behindertenverband-leipzig.de/places/{ID}-entrance" ;
        rr:termType rr:IRI
    ] ;

    rr:predicateObjectMap [
        rr:objectMap [ rr:column "\"Eingangsbereich-rollstuhlgerecht\"" ] ; rr:predicate acc:suitableForWheelchairs
    ] ;

    rr:predicateObjectMap [
        rr:objectMap [ rr:column "\"Kleinste-Tuerbreite-bis-Erreichen-der-Einrichtung-cm\"" ] ; rr:predicate wa:entranceDoorWidth
    ] ;

    rr:predicateObjectMap [
        rr:objectMap [ rr:column "\"Stufen-bis-Eingang-vorhanden\"" ] ; rr:predicate p:stepsTillEntranceAvailable
    ] ;

    rr:predicateObjectMap [
        rr:objectMap [ rr:column "\"Anzahl-der-Stufen-bis-Eingang\"" ] ; rr:predicate p:numberOfStepsTillEntrance
    ] ;

    rr:predicateObjectMap [
        rr:objectMap [ rr:column "\"Hoehe-einer-Stufe-bis-Eingang-cm\"" ] ; rr:predicate p:heightOfAStepTillEntrance
    ] ;

    rr:predicateObjectMap [
        rr:objectMap [ rr:column "\"Eingang-Farbliche-Markierung-erste-u-letzte-Stufe-vorhanden\"" ] ; rr:predicate p:stepsTillEntranceFirstLastColorized
    ] ;

    rr:predicateObjectMap [
        rr:objectMap [ rr:column "\"Eingang-Klingel-vorhanden\"" ] ; rr:predicate p:bellAvailable
    ] ;

    rr:predicateObjectMap [
        rr:objectMap [ rr:column "\"Eingang-Ort-der-Klingel\"" ] ; rr:predicate p:locationOfTheBell
    ] ;

    rr:predicateObjectMap [
        rr:objectMap [ rr:column "\"Eingang-Klingel-mit-Wechselsprechanlage-vorhanden\"" ] ; rr:predicate p:bellWithIntercomAvailable
    ] ;

    rr:predicateObjectMap [
        rr:objectMap [ rr:column "\"Eingang-Hoehe-oberster-Bedienknopf-von-Klingel-cm\"" ] ; rr:predicate p:bellHeightOfHeighestButtonCm
    ] ;

    rr:predicateObjectMap [
        rr:objectMap [ rr:column "\"Eingang-Handlauf-durchgehend-links-vorhanden\"" ] ; rr:predicate p:continuousHandrailLeftAvailable
    ] ;

    rr:predicateObjectMap [
        rr:objectMap [ rr:column "\"Eingang-Handlauf-durchgehend-rechts-vorhanden\"" ] ; rr:predicate p:continuousHandrailRightAvailable
    ] ;

    rr:predicateObjectMap [
        rr:objectMap [ rr:column "\"Tuerart-am-Eingang\"" ] ; rr:predicate p:doorType
    ] ;

    rr:predicateObjectMap [
        rr:objectMap [ rr:column "\"Alternativer-Eingang-fuer-Rollstuhlfahrer-vorhanden\"" ] ; rr:predicate acc:altEntranceForWheelchairUserExisting
    ] ;

    rr:predicateObjectMap [
        rr:objectMap [ rr:column "\"Ort-alternativer-Eingang-fuer-Rollstuhlfahrer\"" ] ; rr:predicate acc:locationAltEntranceForWheelchairUser
    ] ;

    rr:predicateObjectMap [
        rr:objectMap [ rr:column "\"Rampe-vor-Eingang-vorhanden\"" ] ; rr:predicate p:rampAvailable
    ] ;

    rr:predicateObjectMap [
        rr:objectMap [ rr:column "\"Laenge-der-Rampe-cm\"" ] ; rr:predicate p:rampLengthCm
    ] ;

    rr:predicateObjectMap [
        rr:objectMap [ rr:column "\"Hoehe-der-Rampe-cm\"" ] ; rr:predicate p:rampHeightCm
    ] ;

    rr:predicateObjectMap [
        rr:objectMap [ rr:column "\"Breite-der-Rampe-cm\"" ] ; rr:predicate p:rampWidthCm
    ] ;

    rr:predicateObjectMap [
        rr:objectMap [ rr:column "\"Rampe-Handlauf-durchgehend-links-vorhanden\"" ] ; rr:predicate p:rampContinuousHandrailLeftAvailable
    ] ;

    rr:predicateObjectMap [
        rr:objectMap [ rr:column "\"Rampe-Handlauf-durchgehend-rechts-vorhanden\"" ] ; rr:predicate p:rampContinuousHandrailRightAvailable
    ] ;

    rr:predicateObjectMap [
        rr:objectMap [ rr:column "\"Rampe-Farbliche-Markierung-an-Beginn-u-Ende-der-Rampe-vorhanden\"" ] ; rr:predicate p:rampStartEndColorized
    ]
    .


#
#                   p:elevator
# bvl:SomePlace -------------> bvl:SomePlace/Elevator -----------------------> ...
# ...
#
[] a rr:TriplesMap;

    rr:logicalTable [
        rr:tableName "\"le-online-extracted-places.csv\""
    ] ;

    rr:subjectMap [
        rr:class p:Elevator ;
        rr:template "https://behindertenverband-leipzig.de/places/{ID}-elevator" ;
        rr:termType rr:IRI
    ] ;

    rr:predicateObjectMap [
        rr:objectMap [ rr:column "\"Aufzug-Ort-Aufenthaltsort-Aufzugsberechtigter\"" ] ; rr:predicate p:locationOfElevatorMaintainer
    ] ;

    rr:predicateObjectMap [
        rr:objectMap [ rr:column "\"Personenaufzug-rollstuhlgerecht\"" ] ; rr:predicate acc:suitableForWheelchairs
    ] ;

    rr:predicateObjectMap [
        rr:objectMap [ rr:column "\"Stufen-bis-Aufzug-in-der-Einrichtung-vorhanden\"" ] ; rr:predicate acc:stepsTillElevatorInPlaceAvailable
    ] ;

    rr:predicateObjectMap [
        rr:objectMap [ rr:column "\"Anzahl-der-Stufen-bis-Aufzug-in-der-Einrichtung\"" ] ; rr:predicate acc:numberOfStepsTillElevator
    ] ;

    rr:predicateObjectMap [
        rr:objectMap [ rr:column "\"Aufzug-Tuerbreite-cm\"" ] ; rr:predicate wa:elevatorDoorWidth
    ] ;

    rr:predicateObjectMap [
        rr:objectMap [ rr:column "\"Aufzug-Breite-Innenkabine-cm\"" ] ; rr:predicate wa:elevatorSpaceWidth
    ] ;

    rr:predicateObjectMap [
        rr:objectMap [ rr:column "\"Aufzug-Tiefe-Innenkabine-cm\"" ] ; rr:predicate wa:elevatorSpaceDepth
    ] ;

    rr:predicateObjectMap [
        rr:objectMap [ rr:column "\"Aufzug-Hoehe-oberster-Bedienknopf-in-Innenkabine-cm\"" ] ; rr:predicate p:elevatorCabineDistanceFromGroundHeighestButtonCm
    ] ;

    rr:predicateObjectMap [
        rr:objectMap [ rr:column "\"Aufzug-Hoehe-oberster-Bedienknopf-außerhalb-cm\"" ] ; rr:predicate p:elevatorOutsideDistanceFromGroundHeighestButtonCm
    ] ;

    rr:predicateObjectMap [
        rr:objectMap [ rr:column "\"Aufzug-Wendekreis-bei-Ausstieg\"" ] ; rr:predicate p:turningCircle
    ]
    .


#
#                   p:toilet
# bvl:SomePlace -------------> bvl:SomeBuilding/Toilet -----------------------> ...
# ...
#
[] a rr:TriplesMap;

    rr:logicalTable [
        rr:tableName "\"le-online-extracted-places.csv\""
    ] ;

    rr:subjectMap [
        rr:class p:Toilet ;
        rr:template "https://behindertenverband-leipzig.de/places/{ID}-toilet" ;
        rr:termType rr:IRI
    ] ;

    rr:predicateObjectMap [
        rr:objectMap [ rr:column "\"Toilette-rollstuhlgerecht\"" ] ; rr:predicate acc:suitableForWheelchairs
    ] ;

    rr:predicateObjectMap [
        rr:objectMap [ rr:column "\"Toilette-mit-Piktogramm-als-Behindertentoilette-gekennzeichnet\"" ] ; rr:predicate acc:markedWithPictogramAsDisabledToilet
    ] ;

    rr:predicateObjectMap [
        rr:objectMap [ rr:column "\"Stufen-bis-Toilette-in-Einrichtung-vorhanden\"" ] ; rr:predicate p:stepsTillToiletInPlaceAvailable
    ] ;

    rr:predicateObjectMap [
        rr:objectMap [ rr:column "\"Anzahl-Stufen-bis-Toilette-in-Einrichtung\"" ] ; rr:predicate p:numberOfStepsTillToiletInPlace
    ] ;

    rr:predicateObjectMap [
        rr:objectMap [ rr:column "\"Hoehe-der-Stufen-bis-Toilette-in-Einrichtung-cm\"" ] ; rr:predicate p:stepsHeightTillToiletInPlace
    ] ;

    rr:predicateObjectMap [
        rr:objectMap [ rr:column "\"Stufen-bis-Toilette:-Handlauf-durchgehend-links-vorhanden\"" ] ; rr:predicate p:stepsTillToiletContinuousHandrailLeftAvailable
    ] ;

    rr:predicateObjectMap [
        rr:objectMap [ rr:column "\"Stufen-bis-Toilette:-Handlauf-durchgehend-rechts-vorhanden\"" ] ; rr:predicate p:stepsTillToiletContinuousHandrailRightAvailable
    ] ;

    rr:predicateObjectMap [
        rr:objectMap [ rr:column "\"Stufen-bis-Toilette-Farbliche-Markierung-erste-u-letzte-Stufe\"" ] ; rr:predicate p:stepsTillToiletFirstLastColorized
    ] ;

    rr:predicateObjectMap [
        rr:objectMap [ rr:column "\"Tuerbreite-der-Toilettenkabine-cm\"" ] ; rr:predicate wa:toiletDoorWidth
    ] ;

    rr:predicateObjectMap [
        rr:objectMap [ rr:column "\"ToilettenTuer-von-außen-entriegelbar\"" ] ; rr:predicate p:toiletDoorUnlockableFromOutside
    ] ;

    rr:predicateObjectMap [
        rr:objectMap [ rr:column "\"Notklingel-in-Toilettenkabine-vorhanden\"" ] ; rr:predicate p:emergencyBellInsideCabinAvailable
    ] ;

    rr:predicateObjectMap [
        rr:objectMap [ rr:column "\"Hoehe-Notklingel-in-Toilettenkabine\"" ] ; rr:predicate p:distanceFromGroundEmergencyBellInsideCabin
    ] ;

    rr:predicateObjectMap [
        rr:objectMap [ rr:column "\"Bewegungsflaeche-vor-WC-Tiefe-cm\"" ] ; rr:predicate p:areaInFrontOfWCDepthCm
    ] ;

    rr:predicateObjectMap [
        rr:objectMap [ rr:column "\"Bewegungsflaeche-vor-WC-Tiefe-cm\"" ] ; rr:predicate p:areaInFrontOfWCDepthCm
    ] ;

    rr:predicateObjectMap [
        rr:objectMap [ rr:column "\"Bewegungsflaeche-vor-WC-Breite-cm\"" ] ; rr:predicate p:areaInFrontOfWCWidthCm
    ] ;

    rr:predicateObjectMap [
        rr:objectMap [ rr:column "\"Bewegungsflaeche-links-vom-WC:-Tiefe-cm\"" ] ; rr:predicate p:areaLeftOfWCDepthCm
    ] ;

    rr:predicateObjectMap [
        rr:objectMap [ rr:column "\"Bewegungsflaeche-links-vom-WC:-Breite-cm\"" ] ; rr:predicate p:areaLeftOfWCWidthCm
    ] ;

    rr:predicateObjectMap [
        rr:objectMap [ rr:column "\"Bewegungsflaeche-rechts-vom-WC:-Tiefe-cm\"" ] ; rr:predicate p:areaRightOfWCDepthCm
    ] ;

    rr:predicateObjectMap [
        rr:objectMap [ rr:column "\"Bewegungsflaeche-rechts-vom-WC:-Breite-cm\"" ] ; rr:predicate p:areaRightOfWCWidthCm
    ] ;

    rr:predicateObjectMap [
        rr:objectMap [ rr:column "\"Stuetzgriff-neben-WC-vorhanden\"" ] ; rr:predicate p:supportHandleAvailable
    ] ;

    rr:predicateObjectMap [
        rr:objectMap [ rr:column "\"Stuetzgriff-neben-WC-links-klappbar\"" ] ; rr:predicate p:toiletLeftSupportHandleIsFoldable
    ] ;

    rr:predicateObjectMap [
        rr:objectMap [ rr:column "\"Stuetzgriff-neben-WC-rechts-klappbar\"" ] ; rr:predicate p:toiletRightSupportHandleIsFoldable
    ] ;

    rr:predicateObjectMap [
        rr:objectMap [ rr:column "\"Aktivierung-Amatur-Waschbecken-Toilettenkabine\"" ] ; rr:predicate p:vanityBasinFittingActivation
    ] .
